  <properties>
    <property resource="config\properties.config"/>
  </properties>
  <settings>
    <setting useStatementNamespaces="false" />
    <setting cacheModelsEnabled="true" />
    <setting validateSqlMap="true" />
  </settings>

  <!--db provider配置文件路径-->
  <providers resource="config\providers.config"/>

  <!--db provider类型及连接串-->
  <database>
    <provider name="ManagedDataAccess4" />
    <dataSource name="SJK" connectionString="Data Source=${datasource};User Id=${userid};Password=${password}"/>
  </database>

  <!--db与Entity的映射文件-->
  <sqlMaps>
    <sqlMap resource="sql\sqlFile.xml" />
  </sqlMaps>
